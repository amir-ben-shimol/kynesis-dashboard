generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum AuthProviderType {
  GOOGLE
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  email    String           @unique
  authType AuthProviderType

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
